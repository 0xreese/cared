{
  "name": "@mindworld/sdk",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react/client": {
      "types": "./dist/react/client.d.ts",
      "import": "./dist/react/client.mjs",
      "require": "./dist/react/client.js",
      "default": "./dist/react/client.js"
    },
    "./react/server": {
      "types": "./dist/react/server.d.ts",
      "import": "./dist/react/server.mjs",
      "require": "./dist/react/server.js",
      "default": "./dist/react/server.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@mindworld/eslint-config": "workspace:*",
    "@mindworld/prettier-config": "workspace:*",
    "@mindworld/tsconfig": "workspace:*",
    "@trpc/server": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.3.6",
    "typescript": "catalog:"
  },
  "prettier": "@mindworld/prettier-config",
  "dependencies": {
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "ai": "catalog:",
    "drizzle-orm": "^0.38.4",
    "react": "^19.0.0",
    "server-only": "^0.0.1",
    "superjson": "2.2.2",
    "uuid": "11.1.0",
    "zod": "3.24.2"
  }
}

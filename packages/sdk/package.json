{
  "name": "@ownxai/sdk",
  "version": "0.1.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./model": {
      "types": "./dist/model/index.d.ts",
      "import": "./dist/model/index.mjs",
      "require": "./dist/model/index.js",
      "default": "./dist/model/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@ownxai/eslint-config": "workspace:*",
    "@ownxai/prettier-config": "workspace:*",
    "@ownxai/tsconfig": "workspace:*",
    "@trpc/server": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.3.6",
    "typescript": "catalog:"
  },
  "prettier": "@ownxai/prettier-config",
  "dependencies": {
    "@ownxai/shared": "workspace:*",
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "ai": "catalog:",
    "react": "^19.0.0",
    "server-only": "^0.0.1",
    "superjson": "2.2.2"
  }
}

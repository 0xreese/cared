{
  "name": "@ownxai/providers",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./embed": "./src/embed.ts",
    "./rerank": "./src/rerank/index.ts",
    "./providers": "./src/providers.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "extract-model-ids": "tsx ./scripts/extract-model-ids.ts",
    "fetch-openrouter-models": "curl -L https://openrouter.ai/api/v1/models -o ./data/openrouter-models.json",
    "fetch-litellm-models": "curl -L https://github.com/BerriAI/litellm/raw/refs/heads/main/model_prices_and_context_window.json -o ./data/model_prices_and_context_window.json"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^2.2.7",
    "@ai-sdk/anthropic": "^1.2.8",
    "@ai-sdk/azure": "^1.3.10",
    "@ai-sdk/cerebras": "^0.2.8",
    "@ai-sdk/cohere": "^1.2.7",
    "@ai-sdk/deepinfra": "^0.2.9",
    "@ai-sdk/deepseek": "^0.2.8",
    "@ai-sdk/fireworks": "^0.2.8",
    "@ai-sdk/google": "^1.2.10",
    "@ai-sdk/google-vertex": "^2.2.13",
    "@ai-sdk/groq": "^1.2.7",
    "@ai-sdk/luma": "^0.1.6",
    "@ai-sdk/mistral": "^1.2.6",
    "@ai-sdk/openai": "^1.3.9",
    "@ai-sdk/openai-compatible": "^0.2.8",
    "@ai-sdk/perplexity": "^1.1.6",
    "@ai-sdk/provider": "^1.1.2",
    "@ai-sdk/replicate": "^0.2.6",
    "@ai-sdk/togetherai": "^0.2.8",
    "@ai-sdk/xai": "^1.2.9",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "ai": "4.3.4",
    "ajv": "^8.17.1",
    "cohere-ai": "^7.16.0"
  },
  "devDependencies": {
    "@ownxai/eslint-config": "workspace:*",
    "@ownxai/prettier-config": "workspace:*",
    "@ownxai/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "^4.19.3",
    "typescript": "catalog:"
  },
  "prettier": "@ownxai/prettier-config"
}

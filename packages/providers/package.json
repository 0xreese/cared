{
  "name": "@cared/providers",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./embed": "./src/embed.ts",
    "./rerank": "./src/rerank/index.ts",
    "./providers": "./src/providers.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "extract-model-ids": "tsx ./scripts/extract-model-ids.ts",
    "fetch-openrouter-models": "pnpm with-env tsx ./scripts/fetch-openrouter-models.ts",
    "fetch-openrouter-models-json": "curl -L https://openrouter.ai/api/v1/models -o ./data/openrouter-models.json",
    "fetch-litellm-models": "curl -L https://github.com/BerriAI/litellm/raw/refs/heads/main/model_prices_and_context_window.json -o ./data/model_prices_and_context_window.json",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "3.0.3",
    "@ai-sdk/anthropic": "2.0.1",
    "@ai-sdk/assemblyai": "^1.0.1",
    "@ai-sdk/azure": "2.0.5",
    "@ai-sdk/cerebras": "1.0.2",
    "@ai-sdk/cohere": "2.0.1",
    "@ai-sdk/deepgram": "^1.0.1",
    "@ai-sdk/deepinfra": "1.0.2",
    "@ai-sdk/deepseek": "1.0.2",
    "@ai-sdk/elevenlabs": "^1.0.1",
    "@ai-sdk/fal": "^1.0.3",
    "@ai-sdk/fireworks": "1.0.2",
    "@ai-sdk/gateway": "1.0.4",
    "@ai-sdk/gladia": "^1.0.1",
    "@ai-sdk/google": "2.0.3",
    "@ai-sdk/google-vertex": "3.0.4",
    "@ai-sdk/groq": "2.0.3",
    "@ai-sdk/hume": "^1.0.1",
    "@ai-sdk/lmnt": "^1.0.1",
    "@ai-sdk/luma": "1.0.1",
    "@ai-sdk/mistral": "2.0.1",
    "@ai-sdk/openai": "2.0.5",
    "@ai-sdk/openai-compatible": "1.0.2",
    "@ai-sdk/perplexity": "2.0.1",
    "@ai-sdk/provider": "2.0.0",
    "@ai-sdk/provider-utils": "3.0.1",
    "@ai-sdk/replicate": "1.0.1",
    "@ai-sdk/revai": "^1.0.1",
    "@ai-sdk/togetherai": "1.0.2",
    "@ai-sdk/vercel": "^1.0.2",
    "@ai-sdk/xai": "2.0.2",
    "@friendliai/ai-provider": "^0.2.6",
    "@openrouter/ai-sdk-provider": "1.1.0",
    "@portkey-ai/vercel-provider": "^2.1.0",
    "@requesty/ai-sdk": "^0.0.9",
    "ai": "catalog:",
    "ai-gateway-provider": "^0.0.11",
    "ajv": "^8.17.1",
    "cohere-ai": "^7.18.0",
    "decimal.js": "^10.6.0",
    "dify-ai-provider": "^0.1.6"
  },
  "devDependencies": {
    "@cared/eslint-config": "workspace:*",
    "@cared/prettier-config": "workspace:*",
    "@cared/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "^4.19.3",
    "typescript": "catalog:"
  },
  "prettier": "@cared/prettier-config"
}

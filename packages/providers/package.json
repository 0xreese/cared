{
  "name": "@cared/providers",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./embed": "./src/embed.ts",
    "./rerank": "./src/rerank/index.ts",
    "./providers": "./src/providers.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "extract-model-ids": "tsx ./scripts/extract-model-ids.ts",
    "fetch-openrouter-models": "curl -L https://openrouter.ai/api/v1/models -o ./data/openrouter-models.json",
    "fetch-litellm-models": "curl -L https://github.com/BerriAI/litellm/raw/refs/heads/main/model_prices_and_context_window.json -o ./data/model_prices_and_context_window.json"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "3.0.0-beta.3",
    "@ai-sdk/anthropic": "2.0.0-beta.3",
    "@ai-sdk/azure": "2.0.0-beta.3",
    "@ai-sdk/cerebras": "1.0.0-beta.2",
    "@ai-sdk/cohere": "2.0.0-beta.2",
    "@ai-sdk/deepinfra": "1.0.0-beta.2",
    "@ai-sdk/deepseek": "1.0.0-beta.2",
    "@ai-sdk/fireworks": "1.0.0-beta.2",
    "@ai-sdk/gateway": "1.0.0-beta.3",
    "@ai-sdk/google": "2.0.0-beta.6",
    "@ai-sdk/google-vertex": "3.0.0-beta.6",
    "@ai-sdk/groq": "2.0.0-beta.2",
    "@ai-sdk/luma": "1.0.0-beta.2",
    "@ai-sdk/mistral": "2.0.0-beta.2",
    "@ai-sdk/openai": "2.0.0-beta.3",
    "@ai-sdk/openai-compatible": "1.0.0-beta.2",
    "@ai-sdk/perplexity": "2.0.0-beta.2",
    "@ai-sdk/provider": "2.0.0-beta.1",
    "@ai-sdk/provider-utils": "3.0.0-beta.2",
    "@ai-sdk/replicate": "1.0.0-beta.2",
    "@ai-sdk/togetherai": "1.0.0-beta.2",
    "@ai-sdk/xai": "2.0.0-beta.2",
    "@openrouter/ai-sdk-provider": "1.0.0-beta.6",
    "ai": "catalog:",
    "ajv": "^8.17.1",
    "cohere-ai": "^7.16.0"
  },
  "devDependencies": {
    "@cared/eslint-config": "workspace:*",
    "@cared/prettier-config": "workspace:*",
    "@cared/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "^4.19.3",
    "typescript": "catalog:"
  },
  "prettier": "@cared/prettier-config"
}

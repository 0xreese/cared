{
  "name": "@ownxai/cli",
  "version": "0.1.0",
  "type": "module",
  "description": "CLI for the ownx AI platform",
  "repository": {
    "type": "git",
    "url": "https://github.com/ownxai/ownx",
    "directory": "packages/cli"
  },
  "author": "ownx team <support@ownx.ai>",
  "license": "MIT",
  "bin": {
    "ownx": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "pnpm rimraf dist",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "build": "pnpm release",
    "prepublishOnly": "pnpm build",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@ownxai/eslint-config": "workspace:*",
    "@ownxai/prettier-config": "workspace:*",
    "@ownxai/tsconfig": "workspace:*",
    "@types/async-retry": "1.4.9",
    "@types/cross-spawn": "6.0.6",
    "@types/node": "^22.13.10",
    "@types/prompts": "2.4.9",
    "@types/tar": "6.1.13",
    "@types/validate-npm-package-name": "4.0.2",
    "@vercel/ncc": "0.38.3",
    "async-retry": "1.3.3",
    "async-sema": "^3.1.1",
    "ci-info": "4.1.0",
    "commander": "13.1.0",
    "conf": "13.1.0",
    "cross-spawn": "7.0.6",
    "eslint": "catalog:",
    "fast-glob": "3.3.3",
    "picocolors": "1.1.1",
    "prettier": "catalog:",
    "prompts": "2.4.2",
    "rimraf": "^6.0.1",
    "tar": "7.4.3",
    "typescript": "catalog:",
    "update-check": "1.5.4",
    "validate-npm-package-name": "6.0.0"
  }
}
